SET PATH TO F:\GPLA
SELECT * FROM F:\GPLA\L4089 WHERE CHOFER = "CASTELNUOV" .AND. PERIODO = 62014;
INTO CURSOR PLANILLA READWRITE
WREG = 0
VACIO = 0
SET STEP ON ON
